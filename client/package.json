{
  "name": "jaxdsp-client",
  "version": "0.1.0",
  "description": "Interactive editor and monitor for JAXdsp",
  "main": "dist/jaxdsp-client.js",
  "scripts": {
    "build:dev": "rm -rf dist && webpack --mode development --watch",
    "build:prod": "rm -rf dist && webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khiner/jaxdsp/tree/main/client"
  },
  "keywords": [
    "jax",
    "dsp",
    "differentiable",
    "audio"
  ],
  "author": "Karl Hiner",
  "license": "MIT",
  "dependencies": {
    "antd": "^4.16.8",
    "css-loader": "^6.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.2.1",
    "webrtc-adapter": "^7.7.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 110,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
