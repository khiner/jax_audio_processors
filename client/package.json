{
  "name": "jaxdsp-client",
  "version": "0.1.0",
  "description": "Interactive editor and monitor for JAXdsp",
  "main": "dist/jaxdsp-client.js",
  "scripts": {
    "build:dev": "rm -rf dist && mkdir dist && ln -s index.js dist/jaxdsp-client.js",
    "build:prod": "rm -rf dist && webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khiner/jaxdsp/tree/main/client"
  },
  "keywords": [
    "jax",
    "dsp",
    "differentiable",
    "audio"
  ],
  "author": "Karl Hiner",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^8.11.0",
    "@react-three/fiber": "^7.0.26",
    "antd": "^4.18.8",
    "d3-scale": "^4.0.1",
    "d3-time": "^3.0.0",
    "d3-time-format": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.138.0",
    "webrtc-adapter": "^8.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.6.0",
    "husky": "^4.3.8",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 110,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
