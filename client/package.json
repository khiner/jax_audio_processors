{
  "name": "jaxdsp-client",
  "version": "0.1.0",
  "description": "Interactive editor and monitor for JAXdsp",
  "main": "dist/index.js",
  "scripts": {
    "build": "watch 'etsc --external:react-dom --external:react' src",
    "build:prod": "etsc --external:react-dom --external:react",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "cd .. && husky install client/.husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/khiner/jaxdsp/tree/main/client"
  },
  "keywords": [
    "jax",
    "dsp",
    "differentiable",
    "audio"
  ],
  "author": "Karl Hiner",
  "license": "MIT",
  "dependencies": {
    "@react-three/drei": "^8.11.0",
    "@react-three/fiber": "^7.0.26",
    "antd": "^4.18.8",
    "d3-scale": "^4.0.1",
    "d3-time": "^3.0.0",
    "d3-time-format": "^4.0.0",
    "source-map-loader": "^3.0.1",
    "three": "^0.138.0",
    "webrtc-adapter": "^8.1.1"
  },
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/react": "^17.0.39",
    "esbuild-node-tsc": "^1.8.3",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.5",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 110,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
